{
	"manifest_version": 3,
	"name": "Your Browsing Analytics",
	"version": "1.0.0",
	"description": "Beautiful visualizations of your browsing history with interactive dashboards",
	"permissions": ["history", "storage", "tabs"],
	"action": {
		"default_popup": "src/popup.html",
		"default_icon": {
			"16": "public/icon-16.png",
			"48": "public/icon-48.png",
			"128": "public/icon-128.png"
		}
	},
	"icons": {
		"16": "public/icon-16.png",
		"48": "public/icon-48.png",
		"128": "public/icon-128.png"
	},
	"background": {
		"service_worker": "background.js"
	}
}
